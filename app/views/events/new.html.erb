<h1>New Event</h1>

<%= form_for @event do |f| %>
	<%= f.label :title %>
	<%= f.text_field :title %>

	<%= f.label :start %>
	<%= f.datetime_field :start %>

	<%= f.label :end %>
	<%= f.datetime_field :end %>

	<%= f.label :allDay %>
	<%= f.text_field :allDay %>

	<%= f.label :user_name %>
	<%= f.text_field :user_name %>

	<%= f.submit "Create Event", class: "btn btn-primary" %>
<% end %>

<%#= render 'form' %>

<%= link_to 'Back', events_path %>

<%= params[:start] %>
<%= params[:end] %>
